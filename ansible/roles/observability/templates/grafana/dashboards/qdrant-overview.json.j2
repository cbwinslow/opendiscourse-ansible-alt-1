{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "name": "Annotations & Alerts"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": 2,
  "panels": [
    {
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 1
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "id": 10,
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "sum(rate(process_cpu_seconds_total{job=~\"qdrant.*\"}[5m])) by (instance) * 100",
          "legendFormat": "{{instance}} - CPU Usage"
        }
      ],
      "title": "Qdrant CPU Usage",
      "type": "timeseries"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "id": 12,
      "targets": [
        {
          "expr": "process_resident_memory_bytes{job=~\"qdrant.*\"}",
          "legendFormat": "{{instance}} - Memory Usage"
        }
      ],
      "title": "Qdrant Memory Usage",
      "type": "timeseries"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "id": 14,
      "targets": [
        {
          "expr": "rate(qdrant_collection_vectors_count[5m])",
          "legendFormat": "{{collection_name}} - Vectors"
        }
      ],
      "title": "Qdrant Vectors Count",
      "type": "timeseries"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "id": 16,
      "targets": [
        {
          "expr": "rate(qdrant_http_requests_total[5m])",
          "legendFormat": "{{method}} {{status}}"
        }
      ],
      "title": "Qdrant HTTP Requests",
      "type": "timeseries"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 16
      },
      "id": 18,
      "targets": [
        {
          "expr": "rate(qdrant_search_requests_total[5m])",
          "legendFormat": "{{collection_name}} - Search Requests"
        }
      ],
      "title": "Qdrant Search Requests",
      "type": "timeseries"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 24
      },
      "id": 20,
      "targets": [
        {
          "expr": "rate(qdrant_search_duration_seconds_sum[5m]) / rate(qdrant_search_duration_seconds_count[5m])",
          "legendFormat": "{{collection_name}} - Search Latency"
        }
      ],
      "title": "Qdrant Search Latency",
      "type": "timeseries"
    }
  ],
  "refresh": "5s",
  "schemaVersion": 36,
  "style": "dark",
  "tags": ["qdrant", "vector", "monitoring"],
  "templating": {
    "list": []
  },
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
    "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
  },
  "timezone": "browser",
  "title": "Qdrant Overview",
  "version": 1,
  "weekStart": ""
}
